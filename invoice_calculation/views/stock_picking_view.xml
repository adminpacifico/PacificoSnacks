<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="stock_picking_view_form" model="ir.ui.view">
            <field name="name">stock.picking.view.form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='location_id']" position="after">
                    <field name="total_net_weight_oc" colspan="2" />
                    <field name="total_laminated_weight_oc" colspan="2" />
                    <field name="total_box_weight_oc" colspan="2" />
                    <field name="total_gross_weight_oc" colspan="2" />
                    <field name="total_box_oc" colspan="2" />

			    </xpath>

          </field>
        </record>
    </data>
</odoo>